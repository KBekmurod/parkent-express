# Server Configuration
NODE_ENV=development
PORT=3000
HOST=localhost

# MongoDB Configuration
MONGODB_URI=mongodb://localhost:27017/parkent-express
MONGODB_TEST_URI=mongodb://localhost:27017/parkent-express-test

# JWT Configuration
JWT_SECRET=your_jwt_secret_key_change_in_production
JWT_EXPIRES_IN=7d
JWT_REFRESH_SECRET=your_refresh_token_secret
JWT_REFRESH_EXPIRES_IN=30d

# Telegram Bot Configuration
TELEGRAM_BOT_TOKEN=your_telegram_bot_token
TELEGRAM_WEBHOOK_URL=
TELEGRAM_WEBHOOK_SECRET=

# Socket.io Configuration
SOCKET_CORS_ORIGIN=http://localhost:3000
SOCKET_PING_TIMEOUT=60000
SOCKET_PING_INTERVAL=25000

# Location Configuration (Parkent, Uzbekistan)
PARKENT_CENTER_LAT=41.299164
PARKENT_CENTER_LON=69.685219
MAX_DELIVERY_RADIUS_KM=10

# Notification Configuration
NOTIFICATION_RETRY_ATTEMPTS=3
NOTIFICATION_RETRY_DELAY=5000

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info
LOG_FILE_PATH=./logs

# Session Configuration
SESSION_TIMEOUT_MINUTES=30

# Payment Configuration
COMMISSION_RATE=0.10
MIN_ORDER_AMOUNT=5000
CURRENCY=UZS

# Admin Configuration
ADMIN_TELEGRAM_ID=your_admin_telegram_id
