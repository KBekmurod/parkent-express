# Server
NODE_ENV=production
PORT=5000

# MongoDB
MONGO_URI=mongodb://localhost:27017/parkent-express

# Telegram Bot
BOT_TOKEN=your_bot_token_from_botfather

# JWT
JWT_SECRET=your_very_secure_random_string_change_this_in_production
JWT_EXPIRE=7d

# Admin
ADMIN_TELEGRAM_ID=123456789

# Socket.io
SOCKET_CORS_ORIGIN=http://localhost:3000

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=5

# Location Validation (Parkent district bounds)
MIN_LATITUDE=41.2
MAX_LATITUDE=41.4
MIN_LONGITUDE=69.6
MAX_LONGITUDE=69.8

# Delivery
DEFAULT_DELIVERY_FEE=5000
